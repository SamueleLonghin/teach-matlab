{
    "name": "Octave Jupyter",
    // "dockerFile": "Dockerfile",
    "build": {
        "dockerfile": "Dockerfile"
    },
    "workspaceFolder": "/workspaces/matlab",
    // "settings": {
    //     "python.pythonPath": "/usr/bin/python3",
    //     "jupyter.notebookFileRoot": "/workspace"
    // },
    // "postCreateCommand": "python3 -m octave_kernel.install --user",
    "postStartCommand": "jupyter notebook --port=8888 --allow-root --ip='*' --NotebookApp.token='' --NotebookApp.password='' --no-browser --kernel octave",
    // "postStartCommand": "jupyter console --kernel octave",
    "customizations": {
        "vscode": {
            "extensions": [
                "toasty-technologies.octave",
                "LucasFA.octaveexecution",
                "tusindfryd.octave-formatter",
                "leafvmaple.octave",
                "paulosilva.vsc-octave-debugger",
                "ms-python.python",
                "ms-toolsai.jupyter"
            ]
        }
    },
    "containerEnv": {
        "DISPLAY": "0.0"
    },
    // "runArgs": [
    //     "--privileged"
    // ]
}